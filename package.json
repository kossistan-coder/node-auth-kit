    "edge.js": "^6.0.2",
    "express": "^4.21.0",
    "flowbite": "^2.5.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "nodemon": "^3.1.4",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11"
  }
}
